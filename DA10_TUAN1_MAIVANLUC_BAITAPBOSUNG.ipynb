   "source": [
    "def calculate_bmi (height, weight):\n",
    "    bmi = weight / (height ** 2)\n",
    "    return bmi\n",
    "\n",
    "height = float(input (\"Nhập chiều cao của bạn (m):\"))\n",
    "weight = float(input (\"Nhập cân nặng của bạn (kg):\"))\n",
    "\n",
    "bmi = calculate_bmi(height,weight)\n",
    "print (\"Chỉ số bmi của bạn \" + str(bmi))"
   ]
  }
